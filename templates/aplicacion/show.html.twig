{% extends 'base.html.twig' %}

{% block title %}Aplicacion{% endblock %}

{% block body %}
    <h1>Aplicacion</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ aplicacion.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ aplicacion.nombre }}</td>
            </tr>
            <tr>
                <th>ApiKey</th>
                <td>{{ aplicacion.apiKey }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ aplicacion.estado ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_aplicacion_index') }}">back to list</a>

    <a href="{{ path('app_aplicacion_edit', {'id': aplicacion.id}) }}">edit</a>

    {{ include('aplicacion/_delete_form.html.twig') }}
{% endblock %}
